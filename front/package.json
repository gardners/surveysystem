{
  "name": "surveysystem",
  "version": "0.3.2",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.11.0",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.11.0",
    "react-input-range": "^1.3.0",
    "react-modal": "^3.11.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "react-transition-group": "^4.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:staging": "sh -ac '. ./.env.staging; react-scripts build'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "xterm -hold -e \"cd ../backend & sudo ./testrun --skip-npm-build\" && react-scripts start",
    "dev-mock": "xterm -hold -e \"./mockserver/start\" & REACT_APP_SURVEYAPI_ENDPOINT=http://localhost:3098 REACT_APP_SURVEY_LIST=foo react-scripts start",
    "coverage": "npm test -- --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "jest-localstorage-mock": "^2.4.0",
    "node-sass": "^4.13.0",
    "react-test-renderer": "^16.11.0"
  }
