{
  "name": "surveysystem",
  "version": "0.3.4",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.6.2",
    "react": "^16.14.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.14.0",
    "react-input-range": "^1.3.0",
    "react-modal": "^3.15.1",
    "react-router-dom": "^5.3.3",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "xterm -hold -e \"cd ../backend & sudo ./testrun --skip-npm-build\" && react-scripts start",
    "dev-mock": "xterm -hold -e \"./mockserver/start\" & REACT_APP_SURVEYAPI_ENDPOINT=http://localhost:3098 REACT_APP_SURVEY_LIST=foo react-scripts start",
    "coverage": "npm test -- --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "env-cmd": "^10.1.0",
    "node-sass": "^7.0.1",
    "react-test-renderer": "^16.14.0"
  },
  "overrides": {
    "react-error-overlay": "6.0.9"
  }
}
